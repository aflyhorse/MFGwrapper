<Window x:Class="MFGwrapper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MFGwrapper"
        mc:Ignorable="d"
        Title="MainWindow" Height="148" Width="258">
    <Grid>
        <Button x:Name="buttonStart" Content="Start" HorizontalAlignment="Left" Margin="37,15,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonStop" Content="Stop" HorizontalAlignment="Left" Margin="142,15,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <Button x:Name="buttonLog" Content="Show Log" HorizontalAlignment="Left" Margin="37,50,0,0" VerticalAlignment="Top" Width="75"/>
        <CheckBox x:Name="checkBoxAutostart" Content="Autostart MFG Next Time" HorizontalAlignment="Left" Margin="37,86,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonSettings" Content="Settings" HorizontalAlignment="Left" Margin="142,50,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
